@{
    ViewData["Title"] = "About";
}
<style>

    body {
        background: #ebebeb;
    }

    .questions .title2 {
        margin-bottom: 43px;
    }

    .block-main {
        display: none;
    }

    .block-title {
        position: relative;
        color: #000;
        width: 775px;
        font-size: 19px;
        line-height: 23px;
        margin-bottom: 10px;
        display: inline-block;
        *display: inline;
        *zoom: 1;
        text-decoration: none;
        cursor: pointer;
    }

        .block-title span {
            border-bottom: 1px dashed #b2b2b2;
        }

        .block-title:hover span, .opened .block-title span {
            border: 0;
        }

    .opened .block-title:before {
        content: "";
        position: absolute;
        left: -23px;
        top: 10px;
        width: 0;
        border: 5px solid transparent;
        border-top: 4px solid #ff9898;
    }


    .outline {
        margin-left: 15%;
        width: 900px;
        border: 1px solid white;
        padding: 0 10px;
        background: #FFFFFF;
    }

    h1 {
        position: relative;
        left: 200px;
    }

    a:hover {
        color: #000088; /* Цвет текста */
    }

    }
</style>
<script>
    function hidetxt(a) {
        a = document.getElementById(a).style;
        a.display = "block" != a.display ? "block" : "none"
    };
</script>
<body>
    <h1>Rules and Frequently Asked Questions</h1>
    <div class="outline">
        <div class="questions-main">
            <div>
                <h2>1. Rules and documents</h2>
                <a onclick="hidetxt('div1');" class="block-title" rel="nofollow" href="#"><span>Terms of use</span></a>
